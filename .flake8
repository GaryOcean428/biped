[flake8]
max-line-length = 88
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    build,
    dist,
    *.egg-info,
    migrations
ignore = 
    E203,
    E501,
    W503,
    F401
per-file-ignores =
    __init__.py:F401,
    */migrations/*:E501,F401
max-complexity = 12
count = True
statistics = True
